{
  "name": "@fetchforms/react",
  "version": "1.1.3",
  "description": "The React client SDK for Fetch Forms",
  "author": "lukefrogger",
  "license": "MIT",
  "keywords": [
    "form builder",
    "react",
    "fetch forms",
    "formik"
  ],
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fetchforms/react-client.git"
  },
  "dependencies": {
    "final-form": "^4.20.4",
    "react-final-form": "^6.5.7",
    "react-number-format": "^4.9.1"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "scripts": {
    "start": "craco start",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "test": "craco test",
    "eject": "react-scripts eject",
    "package": "npm run build && npm publish"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "dotenv": "^16.0.0",
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.0",
    "@craco/craco": "^6.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.16.13",
    "autoprefixer": "^9.8.8",
    "babel-plugin-transform-scss": "^1.1.0",
    "babel-preset-minify": "^0.5.1",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.39",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.1",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
  }
}
